<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">

  <object name="portal_tabs" meta_type="CMF Action Category">
    <object name="index_html" meta_type="CMF Action">
      <property name="visible">False</property>
    </object>
  </object>

  <object name="site_actions" meta_type="CMF Action Category">
    <object name="imprint" meta_type="CMF Action" after="contact" >
      <property name="title">Impressum</property>
      <property name="description">Impressum</property>
      <property name="url_expr">string:${globals_view/navigationRootUrl}/impressum</property>
      <property name="visible">True</property>
    </object>
    <object name="accessibility" meta_type="CMF Action" after="imprint" >
    </object>
    <object name="manage_styles" meta_type="CMF Action" after="accessibility" >
    </object>
  </object>

</object>