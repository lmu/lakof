<?xml version="1.0"?>
<registry>

  <record interface="Products.CMFPlone.interfaces.controlpanel.ISiteSchema"
          name="plone.site_title" >
    <value>Landeskonferenz der Frauen- und Gleichstellungsbeauftragten an Bayerischen Hochschulen</value>
  </record>

  <record name="plone.site_logo">
    <value></value>
  </record>

  <!-- the intranet bundle -->
  <records prefix="plone.bundles/lakof-bundle"
           interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="compile">True</value>
    <value key="csscompilation">++plone++lakof.theme/lakof.css</value>
    <value key="jscompilation">++plone++lakof.theme/lakof.js</value>
    <value key="last_compilation"></value>
  </records>

  <record name="plone.email_from_address">
    <value>frauenbeauftragte@lmu.de</value>
  </record>

  <record name="plone.email_from_name">
    <value>Webmaster LaKoF Portal</value>
  </record>

  <record name="plone.smtp_host">
    <value>mailout.lrz.de</value>
  </record>

  <record name="plone.show_excluded_items">
    <value>False</value>
  </record>

  <record name="plone.enable_sitemap">
    <value>True</value>
  </record>

  <record name="plone.exposeDCMetaTags">
    <value>True</value>
  </record>

  <record name="plone.icon_visibility">
    <value>authenticated</value>
  </record>

  <record name="plone.navigation_depth">
      <value>2</value>
  </record>

  <record name="plone.available_languages">
    <value>
      <element>de</element>
    </value>
  </record>

  <record name="plone.default_language">
    <value>de</value>
  </record>

  <record name="plone.default_page_types">
    <value>
      <element>Document</element>
      <element>News Item</element>
      <element>Collection</element>
    </value>
  </record>

  <record interface="Products.CMFPlone.interfaces.controlpanel.ILinkSchema"
          name="plone.mark_special_links">
    <value>False</value>
  </record>

  <record name="plone.formats"
          interface="Products.CMFPlone.interfaces.controlpanel.ITinyMCESchema"
          field="formats">
    <value>{
"discreet": { "inline": "span", "classes": "discreet" },
"clearfix": { "block": "div", "classes": "clearfix" },
"underline": { "inline": "span", "classes": "underline", "exact": "true"}
}
    </value>
  </record>

</registry>
